{
  "manifest_version": 3,
  "name": "Headline Tracker",
  "version": "1.0",
  "description": "Track changes in your LinkedIn network's headlines",
  "content_scripts": [
    {
      "js": [
        "scraper.js"
      ],
      "matches": [
        "https://www.linkedin.com/*"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs"
  ]
}